<h1>Add Station</h1>

<div class="row">
  <div class="span6 offset3">
	<%= form_for(@station) do |f| %>
	  <%= render 'shared/error_messages' %>

	  <%= f.label :name %><br />
	  <%= f.text_field :name %><br />

	  <%= f.label :index %><br />
	  <%= f.number_field :index %><br />

	  <%= f.submit "Create Station", class: "btn btn-large btn-primary" %>
	<% end %>
  </div>
</div>

